{
  "name": "ai-workflow-manager",
  "version": "0.2.20241115",
  "description": "AI-powered workflow management tool with GUI and CLI",
  "main": "dist/main/main/main.js",
  "author": "Actual Outcomes LLC",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run rebuild:sqlite:electron && wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main && npm run build:preload",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.main.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "package": "npm run build && electron-builder",
    "package:win": "npm run build && electron-builder --win",
    "package:mac": "npm run build && electron-builder --mac",
    "cli": "node dist/cli/cli/index.js",
    "postinstall": "electron-builder install-app-deps",
    "test": "npm run rebuild:sqlite:test && vitest run",
    "rebuild:sqlite:test": "npm rebuild better-sqlite3 --build-from-source",
    "rebuild:sqlite:electron": "node scripts/rebuild-sqlite-electron.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@radix-ui/react-toast": "^1.2.15",
    "better-sqlite3": "^9.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.0.4",
    "diff": "^5.2.0",
    "docx": "^9.5.1",
    "electron-updater": "^6.1.7",
    "keytar": "^7.9.0",
    "lucide-react": "^0.554.0",
    "openai": "^6.9.0",
    "pdf-lib": "^1.17.1",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.22",
    "commander": "^11.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.1",
    "vitest": "^4.0.9",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.actualoutcomes.ai-workflow-manager",
    "productName": "AI Workflow Manager",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility"
    }
  }
}
